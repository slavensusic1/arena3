---
import kaufland from '../../assets/kaufland.png'
import apis from '../../assets/apis2.jpg'
import pbz from '../../assets/pbz3.jpg'
import addiko from '../../assets/Addiko.svg'
import king from '../../assets/kingICT.png'
import A1 from '../../assets/A1.png'
import iskon from '../../assets/iskon.png'
import luminum from '../../assets/luminum.svg'
import tvbeat from '../../assets/TvBeat.svg'
import unis from '../../assets/unis.png'
import smartnet from '../../assets/smartnet1.jpeg'
import tcom from '../../assets/Tcom.png'
import telemach from '../../assets/telemach.png'
import bilog from '../../assets/bilog.svg'
const lang = getLangFromUrl(Astro.url);
import { getLangFromUrl, useTranslations } from '../../i18n/utils';
const t = useTranslations(lang);
---


<div class=" px-10">
<div class=" sm:pe-6 lg:pe-8 py-10">
    <div class="flex flex-col justify-center items-center py-5">
     <h2 class="text-left font-bold text-4xl md:text-4xl">{t('partners')}</h2>
    </div>
    <div class="flex flex-col items-center justify-center h-full ">
        <div class="container max-w-8xl mx-auto px-2 md:px-3 lg:px-6">
          <div class="grid grid-cols-3 grid-rows-2 gap-4 md:grid-cols-3 lg:grid-cols-4 md:max-w-xl md:mx-auto place-items-center lg:max-w-6xl lg:mx-auto">
            <div class="flex items-center justify-center w-full">
              <img src={kaufland.src} alt="kaufland logo" style="aspect-ratio:89/56" class="object-contain w-full h-full rounded-md border-2 border border-black  shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
              <img src={pbz.src} alt="kaufland logo" style="aspect-ratio:89/56" class="object-contain w-full h-full rounded-md border-2 border border-black  shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
                <img src={A1.src} alt="kaufland logo" style="aspect-ratio:89/56" class="object-contain py-1 w-full h-full rounded-md border border-black border-2 shadow-md">
              </div>
            <div class="flex items-center justify-center w-full">
                <img src={king.src} alt="kaufland logo" style="aspect-ratio:89/56" class="object-cover p-2 w-full h-full rounded-md border border-black   border-2 shadow-md">
              </div>
                    <div class="flex items-center justify-center w-full">
              <img src={luminum.src} alt="luminum logo" style="aspect-ratio:89/56" class="object-contain px-3 w-full h-full rounded-md border border-black  border-2 shadow-md">
            </div>

              <div class="flex items-center justify-center w-full">
                <img src={bilog.src} alt="rba logo" style="aspect-ratio:89/56" class="p-2 object-contain w-full h-full rounded-md border-2 border border-black shadow-md">
              </div>

            <div class="flex items-center justify-center w-full">
              <img src={telemach.src} alt="apis logo" style="aspect-ratio:89/56" class="object-contain filter  h-full rounded-md border-2 border border-black  border border-black shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
              <img src={tcom.src} alt="pbz logo" style="aspect-ratio:89/56" class="object-contain px-2 w-full h-full rounded-md border-2 border border-black  shadow-md">
             </div>
             <div class="flex items-center justify-center w-full">
              <img src={iskon.src} alt="pbz logo" style="aspect-ratio:89/56" class="p-2 object-contain w-full h-full rounded-md border border-black  border-2 border border-black shadow-md">
            </div>
                 <div class="flex items-center justify-center w-full">
              <img src={addiko.src} alt="addiko logo" style="aspect-ratio:89/56" class="p-2 object-contain w-full h-full border border-black  rounded-md border-2 shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
              <img src={tvbeat.src} alt="addiko logo" style="aspect-ratio:89/56" class="p-2 object-contain filter invert w-full h-full rounded-md border-2 shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
              <img src={unis.src} alt="addiko logo" style="aspect-ratio:89/56" class="p-2 object-contain filter invert w-full h-full rounded-md border-2 shadow-md">
            </div>
            <div class="flex items-center justify-center w-full">
              <img src={smartnet.src} alt="smartnet logo" style="aspect-ratio:89/56" class="p-2 object-contain w-full h-full border border-black  rounded-md border-2 shadow-md border-black">
            </div>

          </div>
        </div>
      </div>


