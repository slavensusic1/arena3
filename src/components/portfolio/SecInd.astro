---
import ProjectLayout from '../../layouts/ProjectLayout.astro'
import underConstruction from "../../assets/underConst.svg"
---
<ProjectLayout>
<div class="bg-gray-200 flex items-center justify-center h-[100vh]">
    <div class="text-center">
        <h1 class="text-4xl font-bold mb-4">Under Construction</h1>
        <p class="text-lg text-gray-700 mb-8">We're working on something awesome. Stay tuned!</p>
        <img src={underConstruction.src} alt="Under Construction" class="mx-auto mb-8">

    </div>
</div>
</ProjectLayout>